{
  "name": "Stash Tabs",
  "version": "1.2",
  "description": "A lightweight take on the problem of too many tabs.",
  "permissions": [
    "tabs", "storage"
  ],
  "browser_action": {
      "default_title": "Stash tabs",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  }
}
