{
  "name": "Stash Tabs",
  "version": "1.12",
  "description": "Close the tabs you don't need at the moment and get back to them later. A lightweight solution to the problem of too many tabs.",
  "permissions": [
    "tabs", "storage"
  ],
  "browser_action": {
      "default_title": "Stash tabs",
      "default_icon": "img/icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      }
    }
  },
  "offline_enabled": true
}
